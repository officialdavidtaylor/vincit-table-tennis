{
  "name": "vincit-table-tennis",
  "version": "0.1.0",
  "packageManager": "yarn@1.22.19",
  "private": true,
  "scripts": {
    "codegen": "eval $(grep '^SUPABASE_PROJECT_ID' .env.local) && supabase gen types typescript --project-id \"$SUPABASE_PROJECT_ID\" --schema public > types/supabase.ts",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write ./src",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check ./src",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "test-all": "yarn check-format && yarn check-lint && yarn check-types && yarn build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "@headlessui/tailwindcss": "^0.1.1",
    "@supabase/supabase-js": "^2.0.5",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.2",
    "husky": "^8.0.2",
    "husky-init": "8.0.0",
    "next": "13.0.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.39.1",
    "supabase": "^1.11.7",
    "typescript": "4.8.4"
  },
  "devDependencies": {
    "@netlify/plugin-nextjs": "^4.28.4-appdir.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.2",
    "husky": "^8.0.0"
  }
}
